<Window x:Class="TicTacToeStarter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicTacToeStarter"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition  Width="*"></ColumnDefinition>
            <ColumnDefinition  Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Image Source="/Images/Grid.png" Grid.RowSpan="3" Grid.ColumnSpan="3" Stretch="Fill"/>

        <Image Source="/Images/blank_image.png" Grid.Row="0" Grid.Column="0" Stretch="Uniform"/>
        <Image Source="/Images/blank_image.png" Grid.Row="0" Grid.Column="1" Stretch="Uniform"/>
        <Image Source="/Images/blank_image.png" Grid.Row="0" Grid.Column="2" Stretch="Uniform"/>

        <Image Source="/Images/blank_image.png" Grid.Row="1" Grid.Column="0" Stretch="Uniform"/>
        <Image Source="/Images/blank_image.png" Grid.Row="1" Grid.Column="1" Stretch="Uniform"/>
        <Image Source="/Images/blank_image.png" Grid.Row="1" Grid.Column="2" Stretch="Uniform"/>

        <Image Source="/Images/blank_image.png" Grid.Row="2" Grid.Column="0" Stretch="Uniform"/>
        <Image Source="/Images/blank_image.png" Grid.Row="2" Grid.Column="1" Stretch="Uniform"/>
        <Image Source="/Images/blank_image.png" Grid.Row="2" Grid.Column="2" Stretch="Uniform"/>
    </Grid>

</Window>
